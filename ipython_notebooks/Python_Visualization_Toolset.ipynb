     "input": [
      "import pandas as pd\n",
      "from matplotlib.pyplot import pie, axis, show\n",
      "%matplotlib inline\n",
      "df = pd.DataFrame({\n",
      "    'Sex': ['female', 'male', 'female'],\n",
      "    'Smoke': [1, 1, 1]})\n",
      "sums = df.Smoke.groupby(df.Sex).sum()\n",
      "axis('equal');\n",
      "pie(sums, labels=sums.index);\n",
      "show()"
     ],
     "language": "python",
